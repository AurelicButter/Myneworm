		"prettier": "prettier --write .",
		"prepare": "husky install",
		"dev:ssr": "ng run Myneworm:serve-ssr",
		"serve:ssr": "node dist/Myneworm/server/main.js",
		"build:ssr": "ng build && ng run Myneworm:server",
		"prerender": "ng run Myneworm:prerender"
